<html>
	<head>
		<%= javascript_include_tag "home/index" %>
	</head>
	<body>
		<div>
			<h1> Create User </h1>
			<input type="text" id="user_name" placeholder="Name">
			<input type="text" id="user_email" placeholder="Email">
			<button id="submit_user">Submit</button>
		</div>
		<div>
			<h1> Create Team </h1>
			<input type="text" id="team_name" placeholder="Name">
			<button id="submit_team">Submit</button>
		</div>
		<div>
			<h1> Users </h1>
			<div id='users'>
				<% @users.each do |u| %>
				<p>
					<%= u.name %>
					<%= u.email %>
				</p>
				<% end %>
			</div>
		</div>
		<div>
			<h1> Teams </h1>
			<div id='teams'>
				<% @teams.each do |t| %>
					<%= t.name %>
					<%= t.email %>
				<% end %>
			</div
		</div>
	</body>
</html>